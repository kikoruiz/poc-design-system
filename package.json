{
  "name": "vite-storybook-poc",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && lerna run build",
    "preview": "vite preview",
    "test": "vitest watch",
    "test:run": "vitest run",
    "start": "npm run write-tailwind-conf && npm run write-themes && npm run storybook",
    "storybook:build": "storybook build",
    "storybook:upgrade": "storybook upgrade --prerelease",
    "storybook": "storybook dev -p 6006",
    "lint": "eslint --config .eslintrc.cjs . --ext .ts,.tsx,.js --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "prettier": "prettier --config .prettierrc.json --write \"{packages,src}/**/*.{ts,tsx}\"",
    "typecheck": "tsc",
    "write-themes": "ts-node-esm themeConfig/writeThemes.ts",
    "write-tailwind-conf": "ts-node-esm themeConfig/writeTailwindConfig.ts",
    "postinstall": "husky install"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "npm": "^9.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-live": "^3.1.1",
    "tailwindcss-radix": "^2.6.2"
  },
  "devDependencies": {
    "@radix-ui/react-tabs": "^1.0.1",
    "@storybook/addon-essentials": "^7.0.0-beta.25",
    "@storybook/addon-interactions": "^7.0.0-beta.25",
    "@storybook/addon-links": "^7.0.0-beta.25",
    "@storybook/blocks": "^7.0.0-beta.25",
    "@storybook/react": "^7.0.0-beta.25",
    "@storybook/react-vite": "^7.0.0-beta.25",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.26",
    "@testing-library/dom": "^8.19.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "class-variance-authority": "^0.4.0",
    "copy-to-clipboard": "^3.3.3",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.6.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-testing-library": "5.9.1",
    "hex-rgb": "5.0.0",
    "husky": "8.0.3",
    "jsdom": "^21.0.0",
    "lerna": "^6.1.0",
    "lint-staged": "13.1.0",
    "postcss": "^8.4.20",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^10.2.0",
    "prettier": "2.8.2",
    "prettier-plugin-tailwindcss": "0.2.1",
    "react-element-to-jsx-string": "^15.0.0",
    "storybook": "^7.0.0-beta.25",
    "storybook-addon-themes": "^6.1.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.27.1"
  },
  "workspaces": [
    "packages/components/*"
  ]
}
